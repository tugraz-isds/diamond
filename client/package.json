{
  "name": "@diamond/client",
  "version": "3.0.0-SNAPSHOT",
  "description": "Diamond Frontend Application",
  "scripts": {
    "ng": "ng",
    "watch": "ng build --watch --configuration development",
    "postinstall": "cd .. && node extract-version-info.js",
    "start": "ng serve",
    "start:webpack": "ng serve --extra-webpack-config webpack.partial.js",
    "prebuild": "cd .. && node extract-version-info.js",
    "build": "ng build -c production",
    "build:webpack": "ng build -c production --extra-webpack-config webpack.partial.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.12",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.12",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular/cli": "~16.2.1",
    "@angular/compiler-cli": "^16.2.0",
    "typescript": "~5.1.3"
  }
}